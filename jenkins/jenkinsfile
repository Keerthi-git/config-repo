pipeline {
agent any
stages {
        stage('Cloning')
        {
            steps {
                    script {
                        buildNo=env.BUILD_NUMBER
                        jobName=env.JOB_NAME
                        prop=readProperties file: 'properties/pipeline.properties'
                        print prop.UTIL_REPO
                        sh prop.UTIL_REPO
                        cicdUtil=load prop.JAVA_CI_CD
                        common=load prop.COMMON
                        cicdUtil.execute();
                        } 
                }
          } 
     }
     
    post {
            always {
                script {
                      
                      common.cleanWorkspace();
                 }
            }
     }
}
