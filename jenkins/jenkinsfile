node
{
    stage('loading properties')
    {
         prop=readProperties file: 'config-repo/properties/pipeline.properties'
        echo "load success"
    } 
    
    stage('checkout')
    {
        git url:prop.URL
            branch:prop.BRANCH
        
        echo "checkout success"
     }
     
     stage('mavne build')
     {
       sh mvn clean install
    }
    
    stage('tomact deployment')
    {
        sh cp prop.WAR_FILE+' '+prop.DEPLOY_LOCATION
        echo "deploy success"
    } 
     
}
