pipeline {
agent any
stages {
        stage('Cloning')
        {
            steps {
                    script {
                        buidNo=env.BUILD_NUMBER
                        jobName=env.JOB_NAME
                        prop=readProperties file: 'properties/pipeline.properties'
                        print prop.UTIL_REPO
                        sh prop.UTIL_REPO
                        def ciCdUtil=load prop.JAVA_CI_CD
                        def common=load prop.COMMON
                        cicdUtil.execute();
                        } 
                }
        } 
     }
}
