node
{
    stage('checkout')
    {
        git url:'https://github.com/Keerthi-git/sample.git'
        
        echo "success"
    }
    
    stage('Sonarqube analysis')
    {
              withSonarQubeEnv('qube-1') {
                 sh 'mvn sonar:sonar -Dsonar.host.url=http://my84839dns.eastus2.cloudapp.azure.com:9000'
              }   
    }
    
   
}
