node
{
    stage('checkout')
    {
        git url:'https://github.com/Keerthi-git/sample.git'
        
        echo "success"
    }
    
  
    
    stage('maven build')
    {
        sh 'mvn clean install'
    }
    
    stage('tomact deployment')
    {
        sh 'cp target/*.war /home/minduseradmin/apache-tomcat-8.5.35/webapps
    }
   
}
